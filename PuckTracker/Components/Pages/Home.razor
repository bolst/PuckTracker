@page "/"
@inject NavigationManager navManager

@code {
    static bool firstRender = true;
    protected override void OnInitialized()
    {
        if (firstRender)
        {
            navManager.NavigateTo("/rosters");
            firstRender = false;
        }
        else
        {
            navManager.NavigateTo("/gamesheet");
        }
    }
}